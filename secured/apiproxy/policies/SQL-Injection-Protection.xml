<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RegularExpressionProtection continueOnError="false" enabled="true" name="SQL-Injection-Protection">
  <DisplayName>SQL Injection Protection</DisplayName>
  <Properties/>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <Source>request</Source>
  <QueryParam name="studentId">
    <Pattern>[\s]*((delete)|(exec)|(drop\s*table)|(insert)|(shutdown)|(update)|(\bor\b)|(;))</Pattern>
  </QueryParam>
</RegularExpressionProtection>
